   "source": [
    "from dotenv import load_dotenv\n",
    "from openai import OpenAI\n",
    "\n",
    "load_dotenv()\n",
    "\n",
    "client = OpenAI()\n",
    "\n",
    "resp = client.responses.create(\n",
    "    model=\"gpt-4.1-mini\",\n",
    "    input=\"Hello from Jupyter\"\n",
    ")\n",
    "\n",
    "print(resp.output_text)\n"
   ]
